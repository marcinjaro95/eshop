<div class="super_container">

	<!-- Home -->

	<div class="home">
		<div class="home_container">
			<div class="home_background" style="background-image:url(images/contact.jpg)"></div>
			<div class="home_content_container">
				<div class="container">
					<div class="row">
						<div class="col">
							<div class="home_content">
								<div class="breadcrumbs">
									<ul>
										<li><a href="/">Strona główna</a></li>
										<li>Moje konto</li>
									</ul>
								</div>
							</div>
						</div>
					</div>
				</div>
			</div>
		</div>
	</div>

	<!-- Contact -->

	<div class="contact">
		<div class="container">
			<div class="row">
				<div class="col-lg-2 ">
          <div class="signup_info">
              <div class="signup_section">
                  <div class="button">
                    <a href="/edit-account-data"><span>Dane konta</span></a>
                  </div>
                  <div class="button">
                    <a href="/edit-address-data"><span>Dane adresowe</span></a>
                  </div>
                  <div class="button">
                    <a href="/order-history"><span>Historia zamówień</span></a>
                  </div>
              </div>
            </div>
				</div>

				<!-- Contact Info -->
				<div class="offset-lg-1 col-lg-9 contact_col">
          	<div class="get_in_touch">
						<div class="section_title">Dane konta</div>
						<div class="section_subtitle">Uzupełnij pola formularza</div>
						<div class="contact_form_container">
							<form action="#" id="signup_form" class="contact_form">
								<div class="row">
									<div class="col-xl-6">
										<!-- Name -->
										<label for="contact_name">Imię</label>
										<input type="text" name="profile_name" class="contact_input">
								  </div>
                  <div class="col-xl-6">
                  <!-- Name -->
                    <label for="contact_name">Nazwisko</label>
                    <input type="text" name="profile_surname" class="contact_input">
                  </div>
								</div>
                <div class="row">
                  <div class="col-xl-6">
                  <!-- Name -->
                    <label for="contact_name">Nowe hasło</label>
                    <input type="password" name="profile_password" class="contact_input">
                  </div>
                   <div class="col-xl-6">
                  <!-- Name -->
                    <label for="contact_name">Powtórz nowe hasło</label>
                    <input type="password" name="profile_password_confirm" class="contact_input">
                  </div>
								</div>
                  <div class="row">
                  <div class="col-xl-6">
                  <!-- Name -->
                    <label for="contact_name">Email</label>
                    <input type="text" name="profile_email" class="contact_input">
                  </div>
                    <div class="col-xl-6">
                  <!-- Name -->
                    <label for="contact_name">Nazwa firmy</label>
                    <input type="text" name="profile_firm" class="contact_input">
                  </div>
								</div>
                <div class="row">
                  <div class="col-xl-6">
                  <!-- Name -->
                    <label for="contact_name">REGON</label>
                    <input type="text" name="profile_regon" class="contact_input">
                  </div>
                   <div class="col-xl-6">
                  <!-- Name -->
                    <label for="contact_name">NIP</label>
                    <input type="text" name="profile_nip" class="contact_input">
                  </div>
								</div>
                <div class="row">
                  <div class="col-xl-6">
                  <!-- Name -->
                    <label for="contact_name">Nr telefonu</label>
                    <input type="text" name="profile_phone_number" class="contact_input">
                  </div>
                  <div class="col-xl-6">
                  <!-- Name -->
                    <label for="contact_name">Dodatkowy nr telefonu</label>
                    <input type="text" name="profile_extra_phone_number" class="contact_input">
                  </div>
								</div>
                <div class="row">
                  <div class="col-xl-6">
                  <!-- Name -->
                    <label for="contact_name">Fax</label>
                    <input type="text" name="profile_fax" class="contact_input">
                  </div>
                   <div class="col-xl-6">
                  <!-- Name -->
                    <label for="contact_name">Strona www</label>
                    <input type="text" name="profile_www" class="contact_input">
                  </div>
								</div>
								<button class="button contact_button"><span>Zapisz</span></button>
							</form>
						</div>
					</div>
				</div>
			</div>
		</div>
	</div>

</div>
